    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@beenest/eslint-config": "workspace:*",
    "@beenest/typescript-config": "workspace:*",
    "@types/node": "^24.4.0",
    "@types/react": "19.1.13",
    "eslint": "^9.35.0",
    "react": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "tailwind-merge": "^3.3.1"
  }
}
